--player code/draw calls

function make_player()
	player={}
	player.x=24
	player.y=60
	player.dy=0
	player.rise=1
	player.fall=2
	player.dead=3
	player.speed=2
	player.score=0
	player.jump_velocity=1
end

function draw_player()
	if (game_over) then
		spr(player.dead,player.x,player.y)
	elseif(player.dy < 0) then
		spr(player.rise,player.x,player.y)
	else
		spr(player.fall, player.x, player.y)
	end
end

function move_player()	
	local gravity=0.2 -- how much grav!!
	player_gravity(gravity)
	if (btnp(2)) then
		player_pump()
	end
	-- new pos
	player.y += player.dy
	
	--score
	player.score += flr(player.speed)
	
end

function player_pump()
	--player.dy = 0
	player.dy -= player.jump_velocity
	sfx(0)
end

function player_gravity(grav)
	player.dy+=grav -- gravity!!!
end

function check_hit()

	for i=player.x, player.x+7 do
		if (cave[i+1].top>player.y
			or cave[i+1].btm<player.y+7) then
				game_over=true
				sfx(1)
				-- high score
				if (player.score > high_score) then
					high_score = player.score
					dset(1, high_score)
				end
		end
	end
end

speed_step = 1 -- how much to increment speed
point_step = 1500 -- how many points between speed increment

-- speed up game according to score
function speed_modifier()
	if (next_step < player.score) then
		player.speed += speed_step
		next_step = player.score + point_step
		point_step += point_step / 2 
	end
end

-- either draw current score
-- or end_game
function draw_score()
	if (game_over) then
		print("game over üòê", 38,44,13)
		print("your score:".. player.score, 34, 54, 13)
		print("high score:" ..high_score, 34, 64, 13)             
		print("press ‚ùé to play again..", 18, 74, 11)
		
	else
		print ("score:"..player.score,2,2,13)
	end
end

-- one indexed
-- cuz.. like lua or somethin

selected_item = 1
menu_item_count = 2

menu_states = {}
menu_states.main = 1
menu_states.controls = 2
menu_states.current = menu_states["main"]

function main_menu_logic()
	player_gravity(0.2)
	ai_player_move()
	if (btnp(2)) then
		selected_item -= 1 -- go up
		
	elseif(btnp(3)) then
		selected_item += 1 -- go down
	end
	
	if (selected_item <= 0) then --if @ top, loop around to bottom
			selected_item = menu_item_count
	elseif (selected_item > menu_item_count) then		
		selected_item = 1	
	end
	
	if (menu_states["current"] == menu_states["controls"]) then
		if (btnp(5)) menu_states["current"] = menu_states["main"]
	end
	
	if (btnp(4)) then
	
		if (selected_item == 1) then
			show_menu = false
			make_coin()
		end 
		if (selected_item == 2) menu_states["current"] = menu_states["controls"]
		
	end
	
end

line_colors = {13, 13}
selected_color = 3
default_color = 13

function draw_main_menu()
	draw_player()
	if (menu_states["current"] == menu_states["main"]) then
		line_colors[selected_item] = selected_color
		print("‚óÜflappy pico‚óÜ", 30,70,13)
		print("‚ô• play ‚ô•", 41,44,line_colors[1])
		print("controls", 44, 54, line_colors[2])
		
		line_colors[selected_item] = default_color
	elseif (menu_states["current"] == menu_states["controls"]) then
		print("‚¨ÜÔ∏è: up, ‚ùé: restart or back", 0,44,13)
		print("üÖæÔ∏è: select menu", 0,50,13)
		print("‚óÜby blakers‚óÜ", 33,70,13)
		print("credits: gamedev with pico-8", 10, 60)
	end
end

function ai_player_move()
 player.y += player.dy
	for i=player.x, player.x+7 do
		if (cave[i+1].btm<player.y+ rnd(20,10)) then
			player_pump()
			sfx(0)
		end
	end
end

