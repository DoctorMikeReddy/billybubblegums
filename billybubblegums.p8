-- coins/draw coins


--upper/lower range when to give coins
--range is point based
upper_range = 300
lower_range = 150
coin_points = 500 

-- i am lazy, so only one coin on screen
function make_coin()
 coin={}
 coin.x = 0
 coin.y = 0
 coin.anim = {4, 5, 6, 5} -- animation sprite sequence
 coin.sprite = coin.anim[1] -- current sprite
 coin.anim_counter=0 -- frames in current anim
 coin.frames_per_sprite=4 -- frames before anim change
 next_coin_spawn()
end

function update_coin()
 if (spawn_next <= player.score) then
 	coin.x = #cave
 	
 	local max_btm = 0
 	local min_top = 256
 	local buffer_amnt = 15
 	
 	for i=1, #cave do
			
			if (cave[i].top < min_top) min_top = cave[i].top - buffer_amnt
			if (cave[i].btm > max_btm) max_btm = cave[i].btm + buffer_amnt
 	
 	end
 	
 	coin.y = rnd(max_btm) + (min_top)
 	next_coin_spawn()
	end
 if (coin.x >= 0) then
  anim_coin()
 	coin.x -= player.speed
 end
end

function draw_coin()
	if (coin.x >= 0) then
		spr(coin.anim[coin.sprite], coin.x, coin.y)
	end
end

function anim_coin()
	if (coin.anim_counter < coin.frames_per_sprite) then
		coin.anim_counter += 1
	else
		coin.anim_counter = 0
		coin.sprite += 1
		if (coin.sprite > #coin.anim) coin.sprite=1
	end
end

function hit_coin()
 	local dx = abs(coin.x - player.x)
 	local dy = abs(coin.y - player.y)
 	local length = sqrt(dx*dx + dy*dy)
 	if (length< 8) then
 		coin.x = -1
 		sfx(2)
 		player.score += coin_points
 	end
end

function next_coin_spawn()
	spawn_next = rnd(player.score + lower_range) + (player.score + upper_range)
end
