__lua__
--billy bubblegums

--built on flappy pico by blake

-- main loops

high_score = 0
show_menu = true
cartdata("billybubblegums-v1")

function _init()
 game_over = false
 next_step = point_step -- first speed increment
 make_player()
 make_cave()
 make_coin()
	high_score = dget(1)
end

function _update()
	if (game_over == false) update_cave()
	if (show_menu == true) then
		main_menu_logic()
	elseif (game_over == false) then
		update_coin()
		move_player()
		check_hit()
		hit_coin()
		speed_modifier()
		else
			if (btnp(5)) _init() --restart game
	end
end

function _draw()
	cls()
	draw_bg(7)
	if (show_menu == true) then
		draw_main_menu()
		draw_cave()
	else
		draw_coin()
		draw_cave()
		draw_player()
		draw_score()
	end
end

function draw_bg(col)
	cls()
	rectfill(0, 0, 127, 127, col)
end
